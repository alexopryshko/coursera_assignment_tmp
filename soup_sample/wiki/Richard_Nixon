<source src="//upload.wikimedia.org/wikipedia/commons/transcoded/e/e6/Nixon_resignation_audio_with_buzz_removed.ogg/Nixon_resignation_audio_with_buzz_removed.ogg.mp3" type="audio/mpeg" data-title="MP3" data-shorttitle="MP3" data-transcodekey="mp3" data-width="0" data-height="0" data-bandwidth="124712" />
