                  <mo stretchy="false">|</mo>
                </mrow>
                <mrow class="MJX-TeXAtom-ORD">
                  <mrow class="MJX-TeXAtom-OPEN">
                    <mo maxsize="1.2em" minsize="1.2em">(</mo>
                  </mrow>
                </mrow>
                <mi>A</mi>
                <mrow class="MJX-TeXAtom-ORD">
                  <mo stretchy="false">|</mo>
                </mrow>
