            <mrow>
              <mn>2</mn>
              <mo>&#x00D7;<!-- × --></mo>
              <mn>3</mn>
              <mrow class="MJX-TeXAtom-ORD">
                <mtext>&#xA0;matrix</mtext>
              </mrow>
            </mrow>
          </mover>
        </mrow>
        <mo>=</mo>
        <mrow class="MJX-TeXAtom-ORD">
          <mover>
            <mrow>
              <mo>[</mo>
              <mtable rowspacing="4pt" columnspacing="1em">
                <mtr>
                  <mtd>
                    <mo>&#x22C5;<!-- ⋅ --></mo>
                  </mtd>
                  <mtd>
                    <msub>
                      <mi>x</mi>
                      <mrow class="MJX-TeXAtom-ORD">
                        <mn>12</mn>
                      </mrow>
                    </msub>
                  </mtd>
                  <mtd>
                    <msub>
