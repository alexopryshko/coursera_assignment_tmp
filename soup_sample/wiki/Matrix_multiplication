                          <mo>&#x2217;<!-- ∗ --></mo>
                        </mrow>
                      </msup>
                    </mrow>
                    <mo>]</mo>
                  </mrow>
                  <mrow class="MJX-TeXAtom-ORD">
                    <mi>i</mi>
                    <mi>j</mi>
                  </mrow>
                </msub>
              </mtd>
              <mtd>
