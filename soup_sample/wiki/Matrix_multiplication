                        <mi mathvariant="bold">B</mi>
                      </mrow>
                      <msup>
                        <mo stretchy="false">)</mo>
                        <mrow class="MJX-TeXAtom-ORD">
                          <mrow class="MJX-TeXAtom-ORD">
                            <mi mathvariant="normal">T</mi>
                          </mrow>
                        </mrow>
                      </msup>
                    </mrow>
                    <mo>]</mo>
                  </mrow>
                  <mrow class="MJX-TeXAtom-ORD">
                    <mi>i</mi>
                    <mi>j</mi>
                  </mrow>
                </msub>
              </mtd>
              <mtd>
                <mi></mi>
                <mo>=</mo>
                <msub>
                  <mrow>
