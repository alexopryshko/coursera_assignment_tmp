                    </mrow>
                    <mo>]</mo>
                  </mrow>
                  <mrow class="MJX-TeXAtom-ORD">
                    <mi>i</mi>
                    <mi>j</mi>
                  </mrow>
                </msub>
              </mtd>
              <mtd>
                <mi></mi>
                <mo>=</mo>
                <msub>
