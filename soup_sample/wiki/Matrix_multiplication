                  <msub>
                    <mi>A</mi>
                    <mrow class="MJX-TeXAtom-ORD">
                      <mi>n</mi>
                      <mn>1</mn>
                    </mrow>
                  </msub>
                </mtd>
                <mtd>
                  <msub>
                    <mi>A</mi>
                    <mrow class="MJX-TeXAtom-ORD">
                      <mi>n</mi>
                      <mn>2</mn>
                    </mrow>
                  </msub>
                </mtd>
                <mtd>
                  <mo>&#x22EF;<!-- ⋯ --></mo>
                </mtd>
                <mtd>
                  <msub>
                    <mi>A</mi>
                    <mrow class="MJX-TeXAtom-ORD">
                      <mi>n</mi>
                      <mi>m</mi>
                    </mrow>
                  </msub>
                </mtd>
              </mtr>
            </mtable>
            <mo>)</mo>
          </mrow>
        </mrow>
        <mo>,</mo>
        <mspace width="1em" />
        <mrow class="MJX-TeXAtom-ORD">
          <mi mathvariant="bold">B</mi>
        </mrow>
        <mo>=</mo>
        <mrow class="MJX-TeXAtom-ORD">
          <mrow>
