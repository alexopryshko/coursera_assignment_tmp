                  <mi>i</mi>
                </mrow>
              </msub>
              <mo>,</mo>
              <mrow class="MJX-TeXAtom-ORD">
                <mi mathvariant="bold-italic">&#x03B2;<!-- β --></mi>
              </mrow>
